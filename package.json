{
  "name": "tulio-personal-website",
  "type": "module",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write . && bun run typecheck && bun run build",
    "check:ci": "biome ci . && bun run typecheck && bun run build",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:lint": "biome lint .",
    "biome:format": "biome format --write .",
    "biome:migrate": "biome migrate eslint --write && biome migrate prettier --write",
    "sanity:typegen": "sanity schema extract && sanity typegen generate",
    "sanity:webhook": "node scripts/setup-webhook.js",
    "eslint": "eslint .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.7",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/simple-icons": "^1.2.54",
    "@portabletext/toolkit": "^3.0.1",
    "@portabletext/types": "^2.0.15",
    "@sanity/astro": "^3.2.10",
    "@sanity/client": "^7.12.0",
    "@sanity/code-input": "^6.0.3",
    "@sanity/image-url": "^1.2.0",
    "@sanity/visual-editing": "^3.1.0",
    "@spotlightjs/astro": "^3.2.6",
    "astro": "^5.14.5",
    "astro-portabletext": "^0.11.3",
    "atuin": "^0.2.0",
    "cache": "^3.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "marked": "^16.4.0",
    "mdast-util-from-markdown": "^2.0.2",
    "sanity": "^4.10.3",
    "shiki": "^3.13.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sanity-codegen": "^0.9.8",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.2.22",
  "trustedDependencies": [
    "@sentry/cli"
  ]
}
